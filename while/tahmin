import random

sayi = random.randint(1,100)
hak = 5
sayac = 0

while hak > 0:
  hak -=1
  sayac += 1
  tahmin = int(input('tahmin: '))
  
  if sayi == tahmin:
    print(f'Tebrikler sayıyı {sayac}.tahminde buldunuz:)')
    break
  elif sayi>tahmin:
    print("Tahmininiz düşük kaldı,arttırıp deneyin")
  else:
    print("Tahmininiz yüksek oldu, azaltıp deneyin")
    
  if hak == 0:
    print(f'Hakkınız bitti. Tutulan sayı : {sayi} ')
